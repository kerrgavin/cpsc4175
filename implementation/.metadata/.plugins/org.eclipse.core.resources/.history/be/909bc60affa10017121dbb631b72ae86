import java.util.*;


public class DebitCardGenerator {
	
	private int bitCount;
	
	public static void main(String args[]){
		DebitCardGenerator dbg = new DebitCardGenerator();
		dbg.setBitCount(8);
		System.out.println(dbg.genBitString(dbg.getBitCount()));
	}
	
	public DebitCardGenerator() {
		this(0);
	}
	
	public DebitCardGenerator(int bitCount) {
		setBitCount(bitCount);
	}
	
	public void setBitCount(int bitCount) {
		if(bitCount > 0) {
			this.bitCount = bitCount;
		} else {
			System.out.println("Bit count not large enough");
		}
	}
	
	public int getBitCount() {
		return this.bitCount;
	}
	
	public String genBitString(int bitCount) {
		String bitString = "";
		int seed = 0;
		for(int i = 0; i < bitCount/8; i++) {
			seed = (int) (Math.random() * 128) + 128;
			System.out.println(seed);
			bitString += genByte(seed);
		}
		return bitString;
	}
	
	public String genByte(int seed) {
		if (seed < 10) {
			return ""+(seed%2);
		}
		
		return "" + (seed%2) + genByte(seed/2);
	}
}
